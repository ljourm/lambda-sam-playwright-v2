{
  "name": "lambda-sam-playwright-v2",
  "version": "1.0.0",
  "description": "",
  "license": "UNLICENSED",
  "type": "module",
  "main": "index.js",
  "packageManager": "pnpm@10.19.0",
  "volta": {
    "node": "22.21.0",
    "pnpm": "9.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "unrs-resolver"
    ]
  },
  "scripts": {
    "lint": "run-p lint:*",
    "lint:js": "eslint",
    "lint:fmt": "prettier --check .",
    "lint:ts": "tsc --noEmit",
    "fix": "run-p fix:*",
    "fix:js": "eslint --fix",
    "fix:fmt": "prettier --write .",
    "build": "sam build",
    "deploy:stg:sam": "sam deploy --config-env stg",
    "deploy:prd:sam": "sam deploy --config-env prd",
    "deploy:stg:static": "tsx deployStatic my-stg-bucket src/static/ E1ABCDEF12345",
    "deploy:prd:static": "tsx deployStatic my-prd-bucket src/static/ E1ABCDEF12345",
    "test": "vitest"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.927.0",
    "ajv": "^8.17.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "playwright-core": "^1.56.1"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudfront": "^3.927.0",
    "@aws-sdk/client-s3": "^3.927.0",
    "@eslint/js": "^9.38.0",
    "@sparticuz/chromium": "^141.0.0",
    "@types/aws-lambda": "^8.10.156",
    "@types/node": "^24.9.1",
    "@types/supertest": "^6.0.3",
    "esbuild": "^0.25.12",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "glob": "^11.0.3",
    "globals": "^16.5.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vitest": "^4.0.9"
  }
}
